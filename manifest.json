{
    "name": "EDM Tier Checker",
    "description": "Chrome extension to display a Europeana dataset by EDM Tier",
    "version": "1.0",
    "manifest_version": 3,
    "author": "Digital Repository of Ireland",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["tabs"],
    "action": {
        "default_popup": "popup.html"
    }
}

